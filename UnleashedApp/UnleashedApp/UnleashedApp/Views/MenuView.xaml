<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UnleashedApp.Views.MenuView"
             xmlns:locator="clr-namespace:UnleashedApp"
             BindingContext="{Binding Source={x:Static locator:ViewModelLocator.Instance}, Path=MenuViewModel}">
    <Grid Padding="10"  RowSpacing="5" ColumnSpacing="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Label HorizontalOptions="Center" Grid.Row="0" Text="We're working on this one here" Margin="10"/>
    </Grid>
</ContentPage>